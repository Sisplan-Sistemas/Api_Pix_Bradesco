{
  "name": "Pix",
  "version": "1.0.0",
  "description": "API PIX para realizar ações PIX com o banco Bradesco",
  "main": "./src/index.ts",
  "repository": "https://github.com/rafael-desouza/Pix.git",
  "author": "Rafael Ferreira de Souza <rafael.souza@sisplansistemas.com.br>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn build:clean&&yarn build:dist",
    "build:clean": "rimraf ./dist",
    "build:dist": "tsc --build tsconfig.build.json",
    "start": "set NODE_ENV=production&node ./dist/src/index.js",
    "dev": "set NODE_ENV=development&tsnd -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify ./src",
    "test": "set NODE_ENV=test&jest --i --no-cache --verbose --runInBand --bail",
    "prod": "yarn build&&yarn start",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "class-validator-jsonschema": "^3.1.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.3",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.7",
    "@types/superagent": "^4.1.13",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
